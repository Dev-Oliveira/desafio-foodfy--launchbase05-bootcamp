
<section>
    <div>
        <h3>Avatar</h3>
        <input id="photos-input" type="file" name="photos" multiple="" accept="image/*"
        onchange="PhotosUploadChefs.handleFileChefInput(event)">     
        <i class="material-icons sombra">person_add</i>
        <div id="photos-preview-chefs">
            {% if filechef%}
                <div class="photo">
                   <img src="{{filechef.src}}">
                   <i class="material-icons" onclick="removediv(event)">close</i> 
                </div>
                
            {%endif%}
        </div>         
    </div>
    <div>
        <h3>Nome</h3>
        <input type="text" name="name" value="{{chef.name}}" /> 
                 
    </div>
</section>